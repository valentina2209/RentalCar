const CarList = ({ cars }) => {
  // Створюємо мапу, щоб зберігати тільки унікальні автомобілі за їхнім id
  const uniqueCars = Array.from(new Map(cars.map(car => [car.id, car])).values());

  return (
    <ul className={css.listContainer}>
      {uniqueCars.map((car) => (
        <CarCard key={car.id} car={car} />
      ))}
    </ul>
  );
};